<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	 <!--登陆对话框样式-->
    <drawable name="u8sdk_login_dialog_bg">#00000000</drawable>
    <style name="u8sdk_login_dialog_theme" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/u8sdk_login_shape_corner_round</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
	<!--登陆对话框样式 全屏模式-->
	<style name="u8sdk_login_dialog_theme_full" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@drawable/u8sdk_login_dialog_bg</item>
    </style>
    
</resources>