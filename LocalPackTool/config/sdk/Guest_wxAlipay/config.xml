<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params>
        <param name="SDKData" required="1" showName="SDKData" desc="渠道透传参数" bWriteInManifest="0" bWriteInClient="1" />
         <param name="WX_APP_ID" required="1" showName="微信AppID" desc="应用宝" bWriteInManifest="0" bWriteInClient="1" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="5" type="copyRes" from="root" to="" />
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.WXSDKUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.WXSDKPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>微信SDK</name>
        <versionCode>1</versionCode>
        <versionName>1.0.2</versionName>
    </version>    
    
</config>