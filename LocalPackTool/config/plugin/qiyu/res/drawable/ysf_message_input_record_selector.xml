<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <size android:width="100dp" android:height="36dp" />
            <solid android:color="#e6e9ec" />
            <stroke android:width="1dp" android:color="#b9bfc5" />
            <corners android:radius="100dp" />
        </shape>
    </item>
    <item>
        <shape>
            <size android:width="100dp" android:height="36dp" />
            <solid android:color="@color/ysf_white" />
            <stroke android:width="1dp" android:color="#dadde0" />
            <corners android:radius="100dp" />
        </shape>
    </item>
</selector>